    <style>
        .card {

        }

        .avatar-sm {
            width: 64px !important;
            height: 64px !important;
            font-size: 0.875rem;
        }

        .avatar-sm.avatar-raised {
            margin-top: -18px;
        }

        .table-fixed-header thead {
            top: 0;
            z-index: 2;
        }

        .tooltip-inner {
            max-height: 200px; /* 원하는 최대 높이로 설정 */
            overflow-y: auto;
        }

        #list-recom {
            position: absolute;
        }

        .text-black.text-capitalize.ps-3 {
            color: #747474;
        }

        .table-container {
            max-height: 300px;
            overflow-y: auto;
        }

        .table {
            table-layout: auto;
        }

        .table td, .table th {
            max-width: 100px;
            text-overflow: ellipsis;
            white-space: nowrap;
        }

        table {
            width: 100%;
            border-collapse: collapse;
        }

        th, td {
            padding: 10px;
        }

        thead th {
            position: sticky;
            top: 0;
            background-color: whitesmoke;
        }

        .bg-gradient-primary {
            height: 90px;
            background: whitesmoke;
        }

        #btnStart {
            height: 44px;
        }


        .labelThead.text-white {
            color: #656464 !important;
            background-color: whitesmoke;
            position: sticky;
            z-index: 999;
        }

        .input-group {
            top: 8px;
            margin-left: -18px;
            position: sticky;
            z-index: 1000;
        }

        .containerBtn {

        }

        .tooltip {
            border-radius: 5px;

        }

        .tooltip-inner {
            background: #42424a;
            color: white;
        }

        #modelName {
            font-size: 16px;
            font-weight: bold;
            color: #0071E3;
            margin-top: 14px;
        }

        #text-className {
            font-size: 15px;
        }
        #text-className:hover{
            color: #353535;
        }

        .btnModelName {
            display: block;
            position: static;
            float: left;
            width: 90%;
            padding: 0;
            margin: 10px 20px 10px 0;
            font-weight: 400;
            text-align: center;
            line-height: 25px;
            color: #FFF;
            border-radius: 5px;
            transition: all 0.2s;
            background: #FFAA40;
        }

        .btnFade.btnOrange:hover {
            background: #FF8E00;
        }

        .table-scrollable {
            max-height: 700px;
            overflow-y: auto;
        }

        .text-sm {
            margin-left: 15px;
        }

        #list-recom {
            position: absolute;
        }


        #autocomplete-list {
            border: 1px solid #ccc;
            max-height: 275px !important;
            overflow-y: auto;
            position: absolute;
            margin-top: 40px;
            z-index: 1000;
        }

        #autocomplete-list::-webkit-scrollbar {
            display: none;
        }


        #autocomplete-list div {
            padding: 10px;
            cursor: pointer;
            background-color: white;
            z-index: 1;
        }

        .input-group {
            top: 8px;
            margin-left: -18px;
            position: sticky;

        }


        .suggestSearch {
            color: grey !important;
        }

        #customH6 {
            position: relative;
        }

        #customH6::after {
            content: '';
            display: block;
            width: 0;
            height: 5px;
            position: absolute;
            left: 0;
            bottom: 0px;
            background: rgba(200, 125, 220, .6);
        }

        #goCustomModel:hover #customH6::after {
            width: 260px;
            transition: width .3s;
        }

        #customDiv {
            bottom: 30px;
            right: 100px;
            position: fixed;
        }

        .image-list-container {
            display: none;
            position: absolute;
            left: 0; /* 왼쪽에 표시 */
            width: 200px; /* 너비 조정 */
            /* 추가적인 스타일링 (위치, 크기 등) */
        }

        .image-list-container.active {
            display: block;
        }

    </style>
</head>

