    <link id="pagestyle" href="assets/css/material-dashboard.css?v=3.0.0" rel="stylesheet"/><!-- 부트스트랩 CSS CDN 링크 -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css"
          integrity="sha384-MXlRQWPmo+Ak3bQhMx4S5e7Sl8o3k70l4ZiSn83p1bx2cAY/xKpnPp1x1r4QbIov" crossorigin="anonymous">

    <!-- 부트스트랩 JavaScript 및 Popper.js CDN 링크 -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"
