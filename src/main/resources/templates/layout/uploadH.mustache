        // //로딩 percentage
        function startPrediction(){
            console.log("startPrediction 실행13123123123123123");
            var currentPercentage = 0;
            var totalDuration = 8000;
            var intervalDuration =totalDuration/100;
            setTimeout(function () {
                var loadingInterval = setInterval(function () {
                    if (currentPercentage < 98) {
                        currentPercentage += 1;
                        updateLoading(currentPercentage);
                    } else {
                        clearInterval(loadingInterval);
                    }
                }, intervalDuration);
            }, 2500);
        }
        function updateLoading(percentage){
            console.log("updateLoading부분 실행");
            var loadingPercentageElement = document.getElementById('loadingPercentage');
            loadingPercentageElement.textContent = percentage + "%";
        }
        startPrediction();
    </script>
